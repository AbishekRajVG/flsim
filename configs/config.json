{
    "clients": {
        "total": 50,
        "per_round": 50,
        "label_distribution": "uniform",
        "do_test": false,
        "test_partition": 0.2,
        "selection": "short_latency_high_loss_first"
    },
    "data": {
        "loading": "dynamic",
        "partition": {
            "size": 600,
            "range": [
                50,
                200
            ]
        },
        "IID": false,
        "bias": {
            "primary": 1.0,
            "secondary": false
        }
    },
    "federated_learning": {
        "rounds": 100,
        "target_accuracy": 0.95,
        "task": "train",
        "epochs": 5,
        "batch_size": 100
    },
    "model": "MNIST",
    "paths": {
        "data": "./data",
        "model": "./models",
        "reports": "reports.pkl",
        "plot": "./plots"
    },
    "server": "sync",
    "async": {
        "alpha": 0.9,
        "staleness_func": "polynomial"
    },
    "link_speed": {
        "min": 5000,
        "max": 5000,
        "std": 0
    },
    "plot_interval": 1
}
